class Dogs():
  def __init__(self, name, color, dob):
    self.name = name
    self.color = color
    self.dob = dob
    
  def greet(self):
    print('my name is {}'.format(self.name))
    
peter = Dogs('peter', 'white', '20100405')
cocopuff = Dogs('cocopuff', 'brown', '20121225')

peter.greet()
cocopuff.greet()
